{% extends "base.j2" %}



    {% block content %}
    
    

   <form method="POST" novalidate>
        {{ form.hidden_tag() }}
       <p>
            {{ form.username.label}}<br>
            {{ form.username(size=32) }}
        </p>
        
        <p>
            {{ form.password.label}}<br>
            {{ form.password(size=32) }}
        </p>
        <p>
            {{ form.submit() }}
        </p>
    
    </form>

    {% endblock content %}